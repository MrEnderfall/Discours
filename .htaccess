RewriteEngine on

RewriteRule ^$ /forum/?forum=1

RewriteRule ^topic/([a-z0-9._-]+)$ /forum/?topic=$1
RewriteRule ^forum/([a-z0-9._-]+)$ /forum/?forum=$1

################################################
RewriteRule ^(ph)$ https://discou.rs/$1/ [R,L]
RewriteRule ^(ph)/$ /forum/?forum=2

RewriteRule ^(test)$ https://discou.rs/$1/ [R,L]
RewriteRule ^(test)/$ /forum/?forum=3

RewriteRule ^(po)$ https://discou.rs/$1/ [R,L]
RewriteRule ^(po)/$ /forum/?forum=4

RewriteRule ^(vg)$ https://discou.rs/$1/ [R,L]
RewriteRule ^(vg)/$ /forum/?forum=5

RewriteRule ^(trash)$ https://discou.rs/$1/ [R,L]
RewriteRule ^(trash)/$ /forum/?forum=6
################################################

RewriteRule ^(login)$ /dashboard?page=$1
RewriteRule ^(delete)$ /dashboard?page=$1
RewriteRule ^(backup)$ /dashboard?page=$1

RewriteRule ^(about)$ /page?page=$1
RewriteRule ^(principles)$ /page?page=$1
RewriteRule ^(modlog)$ /page?page=$1
RewriteRule ^(faq)$ /page?page=$1
RewriteRule ^(contact)$ /page?page=$1
RewriteRule ^(developers)$ /page?page=$1

RewriteRule ^(twig)$ /page?page=$1

########################

<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule  ^$ public/    [L]
    RewriteRule  ((?s).*) public/$1 [L]
</IfModule>